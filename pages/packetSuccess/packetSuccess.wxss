page {
  width: 100%;
  height: 100%;
  background: #fff;
}

.wrapper {
  width: 100%;
}

.wrapper .center {
  width: 100%;
}

.wrapper .center .success {
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  margin: 115rpx auto 55rpx;
}

.wrapper .center .success image {
  width: 200rpx;
  height: 200rpx;
}

.wrapper .center .success-tip {
  text-align: center;
  font-size: 40rpx;
  margin-bottom: 45rpx;
}

.wrapper .center .success-tip2 {
  text-align: center;
  font-size: 28rpx;
  color: #888888;
  margin-bottom: 65rpx;
}

.wrapper .btn {
  width: 688rpx;
  height: 90rpx;
  background: #487BF0;
  border-radius: 12rpx;
  color: #fff;
  font-size: 34rpx;
  margin-top: 66rpx;
}

.wrapper .button-hover {
  background: #6a92ef;
}