/*清除浮动*/

.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

page {
  width: 100%;
  height: 100%;
  background: #4e84e5;
}

.wrapper {
  width: 100%;
  height: 100%;
}

.wrapper .zd {
  padding: 0 40rpx;
}

.wrapper .zd .header-img {
  width: 130rpx;
  height: 130rpx;
  margin-bottom: 40rpx;
  border-radius: 50%;
  overflow: hidden;
}

.wrapper .zd .header-img image {
  width: 100%;
  height: 100%;
}

.wrapper .zd .zd-info {
  border-bottom: 2px solid #ffffff;
  padding-bottom: 30rpx;
}

.wrapper .zd .zd-info .info-content .zd-name {
  color: #fff;
  font-size: 30rpx;
}

.wrapper .zd .zd-info .info-content .zd-editor {
  width: 100rpx;
  height: 40rpx;
  background: #a6c1f9;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #ffffff;
  border-radius: 60rpx;
}

.wrapper .zd .zd-info .zd-id {
  font-size: 24rpx;
  color: #e1e8fc;
}

.wrapper .zd .match-info {
  width: 100%;
  margin-top: 30rpx;
}

.wrapper .zd .match-info .match-info-list {
  width: 50%;
  font-size: 24rpx;
  color: #fff;
  margin-bottom: 20rpx;
}

.wrapper .zd .match-info .match-info-list .point {
  color: #ffd800;
}

.wrapper .zd .zd-member {
  width: 670rpx;
  height: 70rpx;
  background: #3256a8;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius: 12rpx;
  margin-bottom: 30rpx;
}

.wrapper .zd .member-ul {
  width: 670rpx;
  height: 568rpx;
}

.wrapper .zd .member-ul .member-list {
  width: 670rpx;
  height: 170rpx;
  box-sizing: border-box;
  background: #e5f0ff;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
  padding: 35rpx 20rpx 30rpx 30rpx;
}

.wrapper .zd .member-ul .member-list .member-list-header {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}

.wrapper .zd .member-ul .member-list .member-list-header .member-list-header-in {
  width: 100rpx;
  height: 100rpx;
  border: 6rpx solid #fff;
  box-sizing: border-box;
  border-radius: 50%;
}

.wrapper .zd .member-ul .member-list .member-list-header .member-list-header-in image {
  width: 100%;
  height: 100%;
}

.wrapper .zd .member-ul .member-list .member-list-header .member-list-name {
  width: 100rpx;
  height: 30rpx;
  color: #fff;
  background: #ffd800;
  font-size: 18rpx;
  text-align: center;
  line-height: 30rpx;
  position: absolute;
  bottom: -5rpx;
  left: 0;
  border-radius: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wrapper .zd .member-ul .member-list .member-list-content {
  display: inline-block;
  vertical-align: middle;
  width: 370rpx;
}

.wrapper .zd .member-ul .member-list .member-list-content .member-content-item {
  float: left;
  width: 50%;
  color: #487bf0;
  font-size: 24rpx;
  margin-top: 16rpx;
}

.wrapper .zd .member-ul .member-list .member-list-rank {
  display: inline-block;
  vertical-align: middle;
  width: 100rpx;
  height: 50rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 30rpx;
  margin-left: 30rpx;
}

.wrapper .zd .member-ul .member-list .member-list-rank.captain {
  background: #ffd800;
}

.wrapper .zd .member-ul .member-list .member-list-rank.crew {
  background: #a6c1f9;
}

.wrapper .fixed-bottom {
  width: 100%;
  height: 128rpx;
  background: #a6c1f9;
  position: fixed;
  bottom: 0;
  left: 0;
}

.wrapper .fixed-bottom .invite-friend {
  width: 670rpx;
  height: 88rpx;
  color: #fff;
  font-size: 42rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 50rpx;
  background: #487bf0;
  margin: 20rpx auto 0;
}

.wrapper .pop-wrapper {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  display: none;
}

.wrapper .pop-wrapper.show {
  display: block;
}

.wrapper .pop-wrapper .pop {
  width: 506rpx;
  height: 530rpx;
  background: #fff;
  border-radius: 18rpx;
  overflow: hidden;
  position: absolute;
  left: 50%;
  margin-left: -253rpx;
  top: 25%;
}

.wrapper .pop-wrapper .pop .pop-header {
  width: 100%;
  height: 110rpx;
  background: #2198ec;
  color: #fff;
  font-size: 40rpx;
  position: relative;
}

.wrapper .pop-wrapper .pop .pop-header image {
  width: 100%;
  height: 100%;
}

.wrapper .pop-wrapper .pop .pop-header text {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  text-align: center;
  line-height: 110rpx;
}

.wrapper .pop-wrapper .pop .pop-body {
  margin-top: 15rpx;
}

.wrapper .pop-wrapper .pop .pop-body .captain-name {
  text-align: center;
  height: 102rpx;
  line-height: 102rpx;
  color: #487bf0;
  font-weight: bold;
  font-size: 30rpx;
  position: relative;
}

.wrapper .pop-wrapper .pop .pop-body .captain-name:after {
  display: block;
  content: '。';
  color: #487bf0;
  font-size: 60rpx;
  position: absolute;
  right: 24%;
  top: -22%;
}

.wrapper .pop-wrapper .pop .pop-body .captain-name:before {
  display: block;
  content: '。';
  color: #487bf0;
  font-size: 60rpx;
  position: absolute;
  left: 31%;
  top: -22%;
}

.wrapper .pop-wrapper .pop .pop-body .captain-name-in {
  color: #666666;
  font-size: 24rpx;
  text-align: center;
}

.wrapper .pop-wrapper .pop .pop-body .name-input {
  width: 426rpx;
  height: 80rpx;
  box-sizing: border-box;
  margin: 20rpx auto 50rpx;
  background: #efefef;
  border-radius: 12rpx;
  overflow: hidden;
  padding-left: 26rpx;
}

.wrapper .pop-wrapper .pop .pop-body .name-input input {
  width: 400rpx;
  height: 80rpx;
  text-align: center;
  text-indent: -26rpx;
  font-size: 30rpx;
}

.wrapper .pop-wrapper .pop .pop-body .submit {
  width: 426rpx;
  height: 80rpx;
  background: #487bf0;
  text-align: center;
  color: #fff;
}

.wrapper .pop-wrapper .close {
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  bottom: 220rpx;
  left: 48%;
}

.wrapper .pop-wrapper .close image {
  width: 100%;
  height: 100%;
}